    "@types/node": "^20.11.20",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "@fastify/jwt": "^8.0.0",
